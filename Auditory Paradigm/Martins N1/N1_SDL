   all_responses = false;
	picture countdown_pic;
	response_active = false;
	code = "calibration";
} countdownTrial;

#trial welcome
trial{
	trial_duration = forever;
	trial_type = first_response;
	all_responses = true;
	stimulus_event{
		picture welcome;
		time = 0;
		response_active = true;
		code = "Welcome";
	} welcomeEvent;
} welcomeTrial;

