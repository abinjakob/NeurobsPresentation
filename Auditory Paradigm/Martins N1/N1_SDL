trial{
	trial_duration = forever;
	trial_type = first_response;
	all_responses = true;
	stimulus_event{
		picture trainingpic;
		time = 0;
		response_active = true;
		code = "trainingtrial";
	} trainingEvent;
} trainingTrial;

#trial end
trial{
	trial_duration = forever;
   trial_type = first_response;
	all_responses = true;
	stimulus_event{
		picture end;
		time = 0;
		response_active = true;
		code = "end";
	} endEvent;
} endTrial;

#trial endtraining
trial{
	trial_duration = forever;
	trial_type = first_response;
	all_responses = true;
	stimulus_event{
		picture endtrainingpic;
		time = 0;
		response_active = true;
		code = "endtraining";
	} endtrainingEvent;
} endtrainingTrial;

