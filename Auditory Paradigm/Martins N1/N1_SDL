	all_responses = false;
	 picture fixation;
    stimulus_event {
    nothing{};
	 response_active = false;
    code = "default";
	 port_code = 4;
    port = 1;
    } defaultevent; 
}defaulttrial;


# picture welcome screen
picture {
   text {caption = "Nun beginnt der eigentliche Versuch \n
Die Abfolge bleibt exakt gleich. Du wirst wieder aufgefordert, per Knopfdruck anzugeben, \n
auf welche Seite du dich konzentrieren möchtest (links oder rechts). \n
Die Tonfolgen sind wie immer entweder aufsteigend (z.B. tief tief hoch),absteigend (z.B. hoch hoch tief), \n
oder wechselnd (z.B. hoch tief hoch). Deine Aufgabe ist es weiterhin, \n
nur auf diese Seite zu hören und die anderen beiden zu ignorieren.\n
Nachdem du die Töne gehört hast, gibst du per Knopfdruck an, ob die Tonfolge auf deiner gewählten Seite \n
aufsteigend, wechselnd, oder absteigend war. Danach folgt der nächste Durchgang. \n 
Vor dem Beginn des Versuchs BLEIBE BITTE EINE MINUTE RUHIG SITZEN.\n
Wenn du bereit bist, drücke ENTER um zu beginnen. Viel Spaß!";
		font_color = 179,179,179;
	}welcomeText;
	x = 0; y = 0; # center
} welcome;


# picture training screen
picture {
   text {caption = "In diesem Experiment hörst du Tonfolgen, die aus drei verschieden Richtungen kommen. \n
Die Tonfolgen sind entweder aufsteigend (z.B. tief tief hoch),\n
absteigend (z.B. hoch hoch tief) oder wechselnd (z.B. hoch tief hoch). \n
Sobald es losgeht, wirst du aufgefordert, per Knopfdruck anzugeben, \n
auf welche Seite du dich konzentrieren möchtest (links oder rechts). \n
Deine Aufgabe ist es dann, nur auf diese Seite zu hören und die anderen beiden zu ignorieren.\n
Nachdem du die Töne gehört hast, gibst du per Knopfdruck an, \n
ob die Tonfolge auf deiner gewählten Seite aufsteigend, wechselnd, oder absteigend war. \n 
Danach folgt der nächste Durchgang. Vor dem Beginn des Versuchs bekommst du einige Übungssätze.\n
Wenn du bereit bist, drücke ENTER um zu beginnen. Viel Spaß!";
		font_color = 179,179,179;
	}trainingText;
	x = 0; y = 0; # center
} trainingpic;

